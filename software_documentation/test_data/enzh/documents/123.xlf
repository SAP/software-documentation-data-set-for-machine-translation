<trans-unit id="efb4cf7e-e7cd-4034-912f-e3b8740d080d">
<source>When using this function:</source>
<seg-source><mrk mtype="seg" mid="6">When using this function:</mrk></seg-source>
<target><mrk mtype="seg" mid="6">使用此功能时：</mrk></target></trans-unit>
<trans-unit translate="no" id="4ac49d47-bfd2-4963-84ba-03bb843be5f4">
