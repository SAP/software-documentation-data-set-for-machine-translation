<trans-unit id="de82d8f2-3ee0-44a4-81b2-755945b97855">
<source><bpt id="18">&lt;userinput&gt;</bpt>Billable<ept id="18">&lt;/userinput&gt;</ept></source>
<seg-source><mrk mtype="seg" mid="7"><bpt id="18">&lt;userinput&gt;</bpt>Billable<ept id="18">&lt;/userinput&gt;</ept></mrk></seg-source>
<target><mrk mtype="seg" mid="7"><bpt id="18">&lt;userinput&gt;</bpt>請求可能<ept id="18">&lt;/userinput&gt;</ept></mrk></target></trans-unit>
<trans-unit translate="no" id="460c6c6e-2723-45c6-a9ca-0489b0137dd3">
<source><ph id="19">&lt;/p&gt;</ph><ph id="20">&lt;p&gt;</ph></source>
