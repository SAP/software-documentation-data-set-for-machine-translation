<trans-unit id="ca51846c-7a8d-40e6-9daa-2a3a034cc28c">
<source>Choose <bpt id="14">&lt;uicontrol&gt;</bpt>Send Email<ept id="14">&lt;/uicontrol&gt;</ept>.</source>
<seg-source><mrk mtype="seg" mid="4">Choose <bpt id="14">&lt;uicontrol&gt;</bpt>Send Email<ept id="14">&lt;/uicontrol&gt;</ept>.</mrk></seg-source>
<target><mrk mtype="seg" mid="4">選擇 <bpt id="14">&lt;uicontrol&gt;</bpt>[傳送電子郵件]<ept id="14">&lt;/uicontrol&gt;</ept>。</mrk></target></trans-unit>
<trans-unit translate="no" id="788c2200-3254-42c4-8698-9918369224c9">
<source><ph id="15">&lt;/p&gt;</ph>
<ph id="16">&lt;/li&gt;</ph>
